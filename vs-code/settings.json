{
  "breadcrumbs.enabled": false,
  "debug.console.fontSize": 14,
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.detectIndentation": false,
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "Consolas ligaturized v3",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.pasteAs.showPasteSelector": "never",
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Tag brackets",
        "scope": [
          "punctuation.definition.tag"
        ],
        "settings": {
          "foreground": "#6b6b6b"
        }
      }
    ]
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "eslint.alwaysShowStatus": true,
  "eslint.codeAction.showDocumentation": {
    "enable": false
  },
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.git": false,
    "**/.gitignore": true,
    "**/.jazzignore": true,
    "**/.svn": false,
    "**/mvn": true,
    "**/target": true,
    ".babelrc.js": true,
    ".dockerignore": true,
    ".editorconfig": true,
    ".eslintignore": true,
    ".jshintrc": true,
    ".npmrc": true,
    ".project": true,
    ".vscode": true,
    "buildWebProject.xml": true,
    "dist_OLD": true,
    "docker-compose.yml": true,
    "jsconfig.json": true,
    "mock/**": true,
    "nginx.conf": true,
    "node_modules": true,
    "node_modules.zip": true,
    "package-lock.json": true,
    "publish.sh": true,
    "web/images/flag/de.svg": true
  },
  "files.insertFinalNewline": true,
  "git-graph.repository.commits.initialLoad": 70,
  "git-graph.repository.commits.loadMoreAutomatically": false,
  "git.autofetch": true,
  "git.autofetchPeriod": 180,
  "git.autoStash": true,
  "git.branchSortOrder": "alphabetically",
  "git.confirmSync": true,
  "git.enabled": false,
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "never",
  "git.postCommitCommand": "push",
  "git.promptToSaveFilesBeforeCommit": "always",
  "git.promptToSaveFilesBeforeStash": "always",
  "git.pullBeforeCheckout": true,
  "git.showProgress": true,
  "git.terminalAuthentication": false,
  "github.gitAuthentication": false,
  "gitlens.advanced.messages": {
    "suppressGitDisabledWarning": true,
    "suppressGitMissingWarning": true
  },
  "gitlens.ai.experimental.generateCommitMessage.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.terminalLinks.enabled": false,
  "gitlens.views.commitDetails.autolinks.enabled": false,
  "gitlens.views.commitDetails.autolinks.enhanced": false,
  "gitlens.views.commitDetails.avatars": false,
  "gitlens.views.fileHistory.avatars": false,
  "gitlens.views.lineHistory.avatars": false,
  "gitlens.views.remotes.avatars": false,
  "gitlens.views.searchAndCompare.avatars": false,
  "gitlens.views.stashes.files.icon": "status",
  "html.hover.documentation": false,
  "html.hover.references": false,
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.enabledFrameworks": [
    "i18next"
  ],
  "java.debug.settings.console": "internalConsole",
  "java.format.comments.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "postman.telemetry.enabled": false,
  "projectManager.sortList": "Name",
  "redhat.telemetry.enabled": false,
  "scm.repositories.sortOrder": "name",
  "scm.showIncomingChanges": "auto",
  "scm.showOutgoingChanges": "auto",
  "search.quickOpen.includeHistory": false,
  "security.workspace.trust.untrustedFiles": "open",
  "task.problemMatchers.neverPrompt": {
    "shell": true
  },
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "Cascadia Mono",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.profiles.windows": {
    "API Server": {
      "args": [
        "-NoLogo"
      ],
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    },
    "PowerShell": {
      "args": [
        "-NoLogo"
      ],
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.stickyScroll.enabled": true,
  "terminal.integrated.tabs.enabled": false,
  "update.mode": "start",
  "vsicons.dontShowNewVersionMessage": true,
  "window.commandCenter": true,
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#ffd700",
    "editorBracketHighlight.foreground2": "#ff8c00",
    "editorBracketHighlight.foreground3": "#d43eaf",
    "editorHoverWidget.border": "#902020",
    "statusBar.background": "#541d1d",
    "statusBar.debuggingBackground": "#831111"
    // Green Color
    // "editorHoverWidget.border": "#246944",
    // "statusBar.background": "#154029",
    // "statusBar.debuggingBackground": "#831111",
    // "statusBar.noFolderBackground": "#154029"
  },
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.languageDetection": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": true
}
